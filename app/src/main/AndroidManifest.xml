<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.fupengpeng.testapp">

    <!-- scrollview -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name="com.test.test01.Activity2" />
        <activity android:name="com.test.test02.OneActivity" />
        <activity android:name="com.test.test02.TwoActivity" />
        <activity android:name="com.test.test03.OneActivity" />
        <activity android:name="com.test.test03.TwoActivity" />
        <activity android:name="com.test.test04.OneActivity" />
        <activity android:name="com.test.listview.ListViewActivity" />
        <activity android:name="com.test.register.LoginActivity" />
        <activity android:name="com.test.register.RegisterActivity" />
        <activity android:name="com.test.register.MainActivity" />
        <activity android:name="com.test.test05.TimeActivity" />
        <activity android:name="com.test.listview.TestListViewActivity" />
        <activity android:name="com.test.listview.ImgListViewActivity" />
        <activity android:name="com.test.animation.AnimActivity" />
        <activity android:name="com.test.animation.AnimShowActivity" />
        <activity android:name="com.test.viewpager.SplashActivity" />
        <activity android:name="com.test.viewpager.GuideActivity" />
        <activity android:name="com.test.viewpager.Main2Activity" />
        <activity android:name="com.test.windowanager.WindowActivity" />
        <activity android:name="com.test.viewpager.MeViewPagerActivity" />
        <activity android:name="com.test.viewpager.WeiBoActivity" />
        <activity android:name="com.test.fragment.Fragment01Activity" />
        <activity android:name="com.test.ScreenClass.activity.ScreenActivity" />
        <activity android:name="com.test.ScreenClass.activity.ExpandableGridViewActivity" />
        <activity android:name="com.test.ScreenClass.activity.ExpandableListViewActivity" />
        <activity android:name="com.test.ScreenClass.activity.ListListActivity" />
        <activity android:name="com.test.ScreenClass.activity.ScrollGridActivity" />
        <activity android:name="com.test.customviewgroup.CustomeViewGroupActivity" />
        <activity android:name="com.test.popupwindow.PopupWindowActivity" />
        <activity android:name="com.test.okhttp.OkHttpActivity" />
        <activity android:name="com.test.json.JSONActivity" />
        <activity android:name="com.test.tabhost.TabHostActivity" />
        <activity android:name="com.test.photograph.PhotographActivity" />

        <!-- 配置实时壁纸Service -->
        <service
            android:name="com.test.scrollview.CameraLiveWallpaper"
            android:label="@string/app_name"
            android:permission="android.permission.BIND_WALLPAPER">

            <!-- 为实时壁纸配置intent-filter -->
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService" />
            </intent-filter>
            <!-- 为实时壁纸配置meta-data -->
            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/livewallpaper" />
        </service>

        <activity android:name="com.test.scrollview.ScrollViewActivity" />
        <activity android:name="com.test.checkbox.CheckBoxActivity" />
        <activity android:name="com.test.radiogroupbadiobutton.RadioGroupButtonActivity" />
        <activity android:name="com.test.spinner.SpinnerActivity" />
        <activity android:name="com.test.progressbar.ProgressBarActivity" />
        <activity android:name="com.test.seekbar.SeekBarActivity"></activity>
        <activity android:name="com.test.gallery.GalleryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.test.service.ServiceActivity"></activity>
    </application>

</manifest>